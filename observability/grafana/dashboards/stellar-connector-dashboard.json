{
  "dashboard": {
    "id": null,
    "title": "Stellar Hummingbot Connector - Production Dashboard",
    "tags": ["stellar", "hummingbot", "trading", "production"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "singlestat",
        "targets": [
          {
            "expr": "stellar_system_uptime_seconds",
            "legendFormat": "Uptime (seconds)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "stellar_health_check_status",
            "legendFormat": "{{component}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": 0, "text": "Unhealthy"},
              {"type": "value", "value": 1, "text": "Degraded"}, 
              {"type": "value", "value": 2, "text": "Healthy"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 2}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Network Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(stellar_network_requests_total[5m])",
            "legendFormat": "{{network}} - {{endpoint_type}}"
          }
        ],
        "yAxes": [
          {"label": "Requests/sec", "min": 0},
          {"show": false}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "API Response Time",
        "type": "graph", 
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(stellar_network_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(stellar_network_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "yAxes": [
          {"label": "Response Time (s)", "min": 0},
          {"show": false}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(stellar_errors_total[5m])",
            "legendFormat": "{{category}} - {{severity}}"
          }
        ],
        "yAxes": [
          {"label": "Errors/sec", "min": 0},
          {"show": false}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.1], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "10s",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data"
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Trading Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(stellar_orders_placed_total[5m])",
            "legendFormat": "Orders {{side}} - {{trading_pair}}"
          }
        ],
        "yAxes": [
          {"label": "Orders/sec", "min": 0},
          {"show": false}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 7,
        "title": "Resource Utilization",
        "type": "graph",
        "targets": [
          {
            "expr": "stellar_resource_utilization_percent",
            "legendFormat": "{{resource_type}} - {{component}}"
          }
        ],
        "yAxes": [
          {"label": "Percentage", "min": 0, "max": 100},
          {"show": false}
        ],
        "thresholds": [
          {"colorMode": "critical", "value": 80},
          {"colorMode": "custom", "value": 90}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 8,
        "title": "Active Connections",
        "type": "singlestat",
        "targets": [
          {
            "expr": "stellar_active_connections",
            "legendFormat": "{{network}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 9,
        "title": "Account Balance",
        "type": "graph",
        "targets": [
          {
            "expr": "stellar_account_balance_xlm",
            "legendFormat": "{{asset_code}} - {{account_id}}"
          }
        ],
        "yAxes": [
          {"label": "XLM Equivalent", "min": 0},
          {"show": false}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 10,
        "title": "Circuit Breaker Status",
        "type": "stat",
        "targets": [
          {
            "expr": "stellar_circuit_breaker_state",
            "legendFormat": "{{network}} - {{endpoint}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": 0, "text": "Closed"},
              {"type": "value", "value": 1, "text": "Open"},
              {"type": "value", "value": 2, "text": "Half-Open"}
            ],
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "red", "value": 1}, 
                {"color": "yellow", "value": 2}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      },
      {
        "id": 11,
        "title": "Cache Hit Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(stellar_cache_hits_total[5m]) / (rate(stellar_cache_hits_total[5m]) + rate(stellar_cache_misses_total[5m]))",
            "legendFormat": "{{cache_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40}
      },
      {
        "id": 12,
        "title": "Alert Firings",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(stellar_alert_firings_total[5m])",
            "legendFormat": "{{alert_rule}} - {{level}}"
          }
        ],
        "yAxes": [
          {"label": "Alerts/sec", "min": 0},
          {"show": false}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40}
      },
      {
        "id": 13,
        "title": "Trading Volume",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(stellar_trading_volume_xlm[5m])",
            "legendFormat": "{{trading_pair}} - {{side}}"
          }
        ],
        "yAxes": [
          {"label": "XLM/sec", "min": 0},
          {"show": false}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 48}
      },
      {
        "id": 14,
        "title": "Order Book Spread",
        "type": "graph",
        "targets": [
          {
            "expr": "stellar_order_book_spread_percent",
            "legendFormat": "{{trading_pair}}"
          }
        ],
        "yAxes": [
          {"label": "Spread %", "min": 0},
          {"show": false}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 48}
      },
      {
        "id": 15,
        "title": "Request Processing Time",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(stellar_request_processing_seconds_bucket[5m])",
            "format": "heatmap",
            "legendFormat": "{{le}}"
          }
        ],
        "heatmap": {
          "xBucketSize": null,
          "yBucketSize": null,
          "yBucketNumber": null
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 56}
      },
      {
        "id": 16,
        "title": "QA - Overall Test Coverage",
        "type": "stat",
        "targets": [
          {
            "expr": "stellar_qa_test_coverage_percentage{coverage_type=\"overall\"}",
            "legendFormat": "{{module}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 85}
              ]
            },
            "custom": {
              "displayMode": "gradient"
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 64}
      },
      {
        "id": 17,
        "title": "QA - Critical Module Coverage",
        "type": "bargauge",
        "targets": [
          {
            "expr": "stellar_qa_test_coverage_percentage{coverage_type=\"critical\"}",
            "legendFormat": "{{module}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            },
            "custom": {
              "orientation": "horizontal",
              "displayMode": "gradient"
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 64}
      },
      {
        "id": 18,
        "title": "QA - Test Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "stellar_qa_test_success_rate",
            "legendFormat": "{{test_suite}} - {{test_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.95},
                {"color": "green", "value": 0.98}
              ]
            },
            "custom": {
              "displayMode": "lcd"
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 64}
      },
      {
        "id": 19,
        "title": "QA - Code Quality Score",
        "type": "gauge",
        "targets": [
          {
            "expr": "stellar_qa_code_quality_score",
            "legendFormat": "{{quality_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 10,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 7},
                {"color": "green", "value": 8.5}
              ]
            },
            "custom": {
              "drawStyle": "line"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 72}
      },
      {
        "id": 20,
        "title": "QA - Compliance Status",
        "type": "stat",
        "targets": [
          {
            "expr": "stellar_qa_compliance_status",
            "legendFormat": "{{requirement_category}} - {{requirement_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": 0, "text": "Non-Compliant"},
              {"type": "value", "value": 1, "text": "Partially Compliant"},
              {"type": "value", "value": 2, "text": "Fully Compliant"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 2}
              ]
            },
            "custom": {
              "displayMode": "table"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 72}
      },
      {
        "id": 21,
        "title": "QA - Test Execution Time Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "stellar_qa_test_execution_time_seconds",
            "legendFormat": "{{test_suite}} - {{test_type}}"
          }
        ],
        "yAxes": [
          {"label": "Execution Time (s)", "min": 0},
          {"show": false}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [300], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "30s",
          "handler": 1,
          "name": "Long Test Execution Time Alert",
          "noDataState": "no_data"
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 80}
      },
      {
        "id": 22,
        "title": "QA - Requirements Coverage Matrix",
        "type": "heatmap",
        "targets": [
          {
            "expr": "stellar_qa_requirement_coverage_percentage",
            "format": "heatmap",
            "legendFormat": "{{requirement_category}}"
          }
        ],
        "heatmap": {
          "xBucketSize": null,
          "yBucketSize": null,
          "yBucketNumber": null,
          "colorMode": "spectrum"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 88}
      },
      {
        "id": 23,
        "title": "QA - Defect Density Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(stellar_qa_defect_count[1h])",
            "legendFormat": "{{severity}} - {{component}}"
          }
        ],
        "yAxes": [
          {"label": "Defects/hour", "min": 0},
          {"show": false}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.5], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "1h", "now"]},
              "reducer": {"type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "High Defect Rate Alert",
          "noDataState": "no_data"
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 88}
      },
      {
        "id": 24,
        "title": "QA - Coverage Trend (7 days)",
        "type": "graph", 
        "targets": [
          {
            "expr": "stellar_qa_test_coverage_percentage{coverage_type=\"overall\"}[7d:1h]",
            "legendFormat": "Overall Coverage"
          },
          {
            "expr": "avg(stellar_qa_test_coverage_percentage{coverage_type=\"critical\"})[7d:1h]",
            "legendFormat": "Critical Modules Avg"
          }
        ],
        "yAxes": [
          {"label": "Coverage %", "min": 75, "max": 100},
          {"show": false}
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 96}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "schemaVersion": 30,
    "version": 1,
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertname=\"DeploymentAlert\"}"
        },
        {
          "name": "QA Test Runs",
          "datasource": "Prometheus", 
          "enable": true,
          "expr": "increase(stellar_qa_test_execution_time_seconds[1h]) > 0",
          "iconColor": "blue",
          "textFormat": "Test Suite: {{test_suite}} ({{test_type}})"
        },
        {
          "name": "Coverage Changes",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "delta(stellar_qa_test_coverage_percentage[1h]) != 0",
          "iconColor": "green",
          "textFormat": "Coverage changed for {{module}}: {{coverage_type}}"
        },
        {
          "name": "QA Alerts",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertname=~\".*QA.*|.*Test.*|.*Coverage.*\"}",
          "iconColor": "red",
          "textFormat": "QA Alert: {{alertname}} - {{severity}}"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "network",
          "type": "query",
          "query": "label_values(stellar_network_requests_total, network)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "trading_pair", 
          "type": "query",
          "query": "label_values(stellar_orders_placed_total, trading_pair)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "test_suite",
          "type": "query",
          "query": "label_values(stellar_qa_test_success_rate, test_suite)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "module",
          "type": "query", 
          "query": "label_values(stellar_qa_test_coverage_percentage, module)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "requirement_category",
          "type": "query",
          "query": "label_values(stellar_qa_compliance_status, requirement_category)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    }
  }
}